    "axios": "^1.3.2",
    "dependencies": "^0.0.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.12.0",
    "music-metadata": "^8.3.0",
    "node-record-lpcm16": "^1.0.1",
    "sync-exec": "^0.6.2",
